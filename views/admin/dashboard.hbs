<div class="container-scroller">

  <!-- partial -->
  <div class="container-fluid page-body-wrapper">
    <!-- partial -->

    <!--sidebar.html -->
    {{>admin-sidebar}}


    <!-- partial -->
    <div class="main-panel">
      <div class="content-wrapper">
        <div class="row">
          <div class="col-sm-12">
            <div class="home-tab">

              <div class="tab-content tab-content-basic">
                <div class="tab-pane fade show active" id="overview" role="tabpanel" aria-labelledby="overview">
                  <div class="row d-flex justify-content-center">
                    <div class="col-sm-10">
                      {{!-- over viewe boxes --}}
                      <div class="statistics-details d-flex align-items-center justify-content-between">
                        <div class="text-center rounded-5 shadow p-2 ps-5 pe-5">
                          <p class="statistics-title">Total Revenue</p>
                          <h3 class="rate-percentage">₹ {{sales.totalRevenue}}</h3>
                        </div>
                        <div class="text-center rounded-5 shadow p-2 ps-5 pe-5">
                          <p class="statistics-title">Total Profit</p>
                          <h3 class="rate-percentage">₹ {{sales.profit}}</h3>
                        </div>
                        <div class="text-center rounded-5 shadow p-2 ps-5 pe-5">
                          <p class="statistics-title">Total Cars</p>
                          <h3 class="rate-percentage">{{carsCount}}</h3>
                        </div>
                        <div class="text-center rounded-5 shadow p-2 ps-5 pe-5">
                          <p class="statistics-title">Total Users</p>
                          <h3 class="rate-percentage">{{usersCount}}</h3>
                        </div>
                        

                      </div>

                    </div>
                  </div>
                  {{!-- charts --}}
                  <div class="row">
                    <div class="col-lg-6 d-flex flex-column">
                      <div class="row flex-grow">
                        <div class="col-12 col-lg-4 col-lg-12 grid-margin stretch-card">
                          <div class="card card-rounded">
                            <div class="card-body">
                              <div class="d-sm-flex justify-content-between align-items-start">
                                <div>
                                  <h4 class="card-title card-title-dash">Trip's completed in last 7 day's</h4>
                
                                </div>
                                <div id="performance-line-legend"></div>
                              </div>
                              <div class="chartjs-wrapper mt-5">
                                <canvas id="performaneLine"></canvas>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-6 d-flex flex-column">
                      <div class="row flex-grow">
                        <div class="col-12 grid-margin stretch-card">
                          <div class="card card-rounded">
                            <div class="card-body">
                              <div class="d-sm-flex justify-content-between align-items-start">
                                <div>
                                  <h4 class="card-title card-title-dash">Sales for last 7 Day's</h4>
                                 
                                </div>
                              </div>
                              <div class="d-sm-flex align-items-center mt-1 justify-content-between">
                                <div class="d-sm-flex align-items-center mt-4 justify-content-between">
                                  <h2 class="me-2 fw-bold">₹ {{oneWeekRevenue}}</h2>
    
                                </div>
                                <div class="me-3">
                                  <div id="marketing-overview-legend"></div>
                                </div>
                              </div>
                              <div class="chartjs-bar-wrapper mt-3">
                                <canvas id="marketingOverview"></canvas>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>

                    </div>
                  </div>
                  {{!-- charts --}}

                  <div class="row d-flex justify-content-center gap-5 mt-5">
                    <div class="text-center">

                    <h2>Trip Completed Status</h2>
                    </div>
                    <div class="col-lg-5 d-flex flex-column">
                      <div class="row flex-grow">
                        <div class="col-12 grid-margin stretch-card">
                          <div class="card card-rounded">
                            <div class="card-body">
                              <div class="row">
                                <div class="col-lg-12">
                                  <div class="d-flex justify-content-between align-items-center mb-3">
                                    <h4 class="card-title card-title-dash">By Top 5 Brand's</h4>
                                  </div>
                                  <canvas class="my-auto" id="doughnutChart" height="200"></canvas>
                                  <div id="doughnut-chart-legend" class="mt-5 ms-3 text-center"></div>
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-5 d-flex flex-column">
                      <div class="row flex-grow">
                        <div class="col-12 grid-margin stretch-card">
                          <div class="card card-rounded">
                            <div class="card-body">
                              <div class="row">
                                <div class="col-lg-12">
                                  <div class="d-flex justify-content-between align-items-center mb-3">
                                    <h4 class="card-title card-title-dash">By Top 5 City's</h4>
                                  </div>
                                  <canvas class="my-auto" id="doughnutChart1" height="200"></canvas>
                                  <div id="doughnut-chart-legend1" class="mt-5 text-center"></div>
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="row d-flex justify-content-center gap-5 mt-5">
                    <div class="text-center">

                    <h2>New Bookings</h2>
                    </div>
                    <div class="col-lg-5 d-flex flex-column">
                      <div class="row flex-grow">
                        <div class="col-12 grid-margin stretch-card">
                          <div class="card card-rounded">
                            <div class="card-body">
                              <div class="row">
                                <div class="col-lg-12">
                                  <div class="d-flex justify-content-between align-items-center mb-3">
                                    <h4 class="card-title card-title-dash">By Top 5 Brand's</h4>
                                  </div>
                                  <canvas class="my-auto" id="doughnutChart2" height="200"></canvas>
                                  <div id="doughnut-chart-legend2" class="mt-5 text-center"></div>
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-5 d-flex flex-column">
                      <div class="row flex-grow">
                        <div class="col-12 grid-margin stretch-card">
                          <div class="card card-rounded">
                            <div class="card-body">
                              <div class="row">
                                <div class="col-lg-12">
                                  <div class="d-flex justify-content-between align-items-center mb-3">
                                    <h4 class="card-title card-title-dash">By Top 5 City's</h4>
                                  </div>
                                  <canvas class="my-auto" id="doughnutChart3" height="200"></canvas>
                                  <div id="doughnut-chart-legend3" class="mt-5 text-center"></div>
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    
      <!-- partial -->
    </div>
    <!-- main-panel ends -->
  </div>
  <!-- page-body-wrapper ends -->
</div>